{
  "identifier": "main",
  "description": "主窗口基础权限（开发阶段）",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "core:window:allow-unmaximize",
    "core:window:allow-toggle-maximize",
    "core:window:allow-close",
    "core:window:allow-start-dragging",
    "core:window:allow-start-resize-dragging",
    "core:window:allow-is-maximized",
    "core:window:allow-is-minimized",
    "core:window:allow-set-focus",
    "core:window:allow-show",
    "core:window:allow-hide",
    "core:window:allow-destroy",
    "core:event:default",
    "core:event:allow-emit",
    "core:event:allow-listen",
    "core:tray:default",
    "core:menu:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        { "url": "https://as.hypergryph.com/**" },
        { "url": "https://as.gryphline.com/**" },
        { "url": "https://binding-api-account-prod.hypergryph.com/**" },
        { "url": "https://binding-api-account-prod.gryphline.com/**" },
        { "url": "https://ef-webview.hypergryph.com/**" },
        { "url": "https://ef-webview.gryphline.com/**" },
        { "url": "https://web-api.hypergryph.com/**" },
        { "url": "https://web-api.gryphline.com/**" },
        { "url": "https://user.hypergryph.com/**" },
        { "url": "https://user.gryphline.com/**" },
        { "url": "https://efgacha.yukiovo.com/**" },
        { "url": "https://efgacha.yukiovo.com/api/**" },
        { "url": "http://localhost:3011/**" },
        { "url": "http://127.0.0.1:3011/**" }
      ]
    },
    {
      "identifier": "http:allow-fetch-cancel",
      "allow": [
        { "url": "https://as.hypergryph.com/**" },
        { "url": "https://as.gryphline.com/**" },
        { "url": "https://binding-api-account-prod.hypergryph.com/**" },
        { "url": "https://binding-api-account-prod.gryphline.com/**" },
        { "url": "https://ef-webview.hypergryph.com/**" },
        { "url": "https://ef-webview.gryphline.com/**" },
        { "url": "https://web-api.hypergryph.com/**" },
        { "url": "https://web-api.gryphline.com/**" },
        { "url": "https://user.hypergryph.com/**" },
        { "url": "https://user.gryphline.com/**" },
        { "url": "https://efgacha.yukiovo.com/**" },
        { "url": "https://efgacha.yukiovo.com/api/**" },
        { "url": "http://localhost:3011/**" },
        { "url": "http://127.0.0.1:3011/**" }
      ]
    },
    {
      "identifier": "http:allow-fetch-read-body",
      "allow": [
        { "url": "https://as.hypergryph.com/**" },
        { "url": "https://as.gryphline.com/**" },
        { "url": "https://binding-api-account-prod.hypergryph.com/**" },
        { "url": "https://binding-api-account-prod.gryphline.com/**" },
        { "url": "https://ef-webview.hypergryph.com/**" },
        { "url": "https://ef-webview.gryphline.com/**" },
        { "url": "https://web-api.hypergryph.com/**" },
        { "url": "https://web-api.gryphline.com/**" },
        { "url": "https://user.hypergryph.com/**" },
        { "url": "https://user.gryphline.com/**" },
        { "url": "https://efgacha.yukiovo.com/**" },
        { "url": "https://efgacha.yukiovo.com/api/**" },
        { "url": "http://localhost:3011/**" },
        { "url": "http://127.0.0.1:3011/**" }
      ]
    },
    {
      "identifier": "http:allow-fetch-send",
      "allow": [
        { "url": "https://as.hypergryph.com/**" },
        { "url": "https://as.gryphline.com/**" },
        { "url": "https://binding-api-account-prod.hypergryph.com/**" },
        { "url": "https://binding-api-account-prod.gryphline.com/**" },
        { "url": "https://ef-webview.hypergryph.com/**" },
        { "url": "https://ef-webview.gryphline.com/**" },
        { "url": "https://web-api.hypergryph.com/**" },
        { "url": "https://web-api.gryphline.com/**" },
        { "url": "https://user.hypergryph.com/**" },
        { "url": "https://user.gryphline.com/**" },
        { "url": "https://efgacha.yukiovo.com/**" },
        { "url": "https://efgacha.yukiovo.com/api/**" },
        { "url": "http://localhost:3011/**" },
        { "url": "http://127.0.0.1:3011/**" }
      ]
    },
    "fs:default",
    "fs:allow-download-read-recursive",
    "fs:allow-download-write-recursive",
    "fs:allow-document-read-recursive",
    "fs:allow-document-write-recursive",
    {
      "identifier": "fs:allow-app-read-recursive",
      "allow": [{ "path": "$APPDATA/**" }]
    },
    {
      "identifier": "fs:allow-app-write-recursive",
      "allow": [{ "path": "$APPDATA/**" }]
    },
    "opener:default",
    "dialog:default",
    "dialog:allow-save",
    "dialog:allow-open",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "updater:default",
    "process:default",
    "process:allow-restart",
    "core:webview:allow-create-webview-window"
  ]
}

